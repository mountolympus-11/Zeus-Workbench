\p
\{\set_style "BLK_BLD_ITAL" buffer-addr\}  Data buffer address.\p
\p
\{\set_style "BLK_BLD_ITAL" pc-number\}  Program call number used for the operation.\p
\p
\{\set_style "MSG_TEXT" Class\}  Geninfo\p
\p
\{\set_style "MSG_TEXT" Module\}  ITCPATRQ                                            
\[----------------------------------------------------------------------\]
\topic "<intmsgt>INTTCP807E"
\0 \0 \{\set_style "MSG" INTTCP807E\} \p
\p
\{\set_style "MSG_TEXT" TCP/IP *unexpected api function*\} \{\set_style "BLK_ITAL" description\}  \{\set_style "MSG_TEXT" api=\}\{\set_style "BLK_ITAL"  call-type\}\p
\{\set_style "MSG_TEXT" air\} \{\set_style "BLK_ITAL" air-addr \}  \{\set_style "MSG_TEXT" rc\}  \{\set_style "BLK_ITAL" return-code\}  \{\set_style "MSG_TEXT" function\}  \{\set_style "BLK_ITAL" function-code\}  \p
