\topic "<intmsgl6>INTL62999I"
\0 \0 \{\set_style "MSG" INTL62999I\}\p
\p
\{\set_style "MSG_TEXT" LU62 tpn start - tkb:\}  \{\set_style "BLK_ITAL" tkb-addr\}  \{\set_style "MSG_TEXT" task:\} \{\set_style "BLK_ITAL" taskname/task-addr\} \{\set_style "MSG_TEXT" proc:\} \{\set_style "BLK_ITAL" procname/proc-addr\}  \{\set_style "MSG_TEXT" tpn:\} \{\set_style "BLK_ITAL" tpname\}\p
